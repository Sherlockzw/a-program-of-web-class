public interface IManDao {
	//增加员工信息
	public void employeeAdd(Employee emp);
	//删除员工信息
	public void employeeDelete(int id);
	//查找员工信息
	public Employee employeeFind(int id);
	//修改员工信息
	public void employeeUpdate(Employee emp);
}
